    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.30",
    "@types/node-fetch": "2",
    "@types/ws": "^8.18.1",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.6.0",
